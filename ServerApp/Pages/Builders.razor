@page "/builders"

<h1>Builders</h1>

@code {
    IComponentBuilder builder = new ComponentBuilder(
        attributes: new Dictionary<string, object> { { "title", "An inline div element" } },
        classes: new[] { "d-inline-block", "border", "border-primary", "m-3", "p-3" },
        styles: new Dictionary<string, string> { { "background-color", "#ccc" }, { "font-family", "Verdana" }, { "font-weight", "700" } }
    );
}
<Div Builder="this.builder">Inline Div</Div>
